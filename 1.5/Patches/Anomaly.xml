<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Anomaly</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/AbilityDef[defName="UnnaturalHealing"]/verbProperties/targetParams/canTargetSelf</xpath>
					<value>
						<canTargetSelf>true</canTargetSelf>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/AbilityDef[defName="PsychicSlaughter"]/verbProperties/targetParams</xpath>
					<value>
						<canTargetSelf>true</canTargetSelf>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/AbilityDef[defName="VoidTerror"]/verbProperties/targetParams/canTargetSelf</xpath>
					<value>
						<canTargetSelf>true</canTargetSelf>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>